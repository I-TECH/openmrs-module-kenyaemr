	<bean id="kenyaemr.hiv.form.moh257ArvTherapy" class="org.openmrs.module.kenyacore.form.FormDescriptor">
		<property name="targetUuid" value="8f5b3ba5-1677-450f-8445-33b9a38107ae" />
		<property name="apps">
			<set>
				<ref bean="kenyaemr.app.chart" />
