	<script type="text/javascript">
		$j(document).ready(function(){
			var pbirthdate = new Date("<lookup expression="patient.birthdate" />").getTime();
			var pweight = "<lookup expression="fn.latestObs(5089).valueNumeric" />"
			var pheight = "<lookup expression="fn.latestObs(5090).valueNumeric" />"
			setValue('weight.value', pweight);
			setValue('height.value', pheight);
			var pulmodiv = document.getElementById('iddivPulmonary');
			var expulmodiv = document.getElementById('expul');
			
			//getting the values of the disease classification option buttons
			var optValues = getValue('dclassification.value');
				if (optValues == 42){
					pulmodiv.style.display = 'inline';
    				expulmodiv.style.display = 'none';
				}
				if (optValues == 5042 ){
					pulmodiv.style.display = 'none';
    				expulmodiv.style.display = 'inline';
				}
			
			$('#dclassification input:radio').click(function() {
			
    			if ($(this).val() == 42) {
    				pulmodiv.style.display = 'inline';
    				expulmodiv.style.display = 'none';
    			}
    			if ($(this).val() == 5042) {
    				pulmodiv.style.display = 'none';
    				expulmodiv.style.display = 'inline';
    			}
			});
		});
	</script>
